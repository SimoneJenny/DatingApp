<Window x:Class="test.sletbruger"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:test"
        mc:Ignorable="d"
        Title="sletbruger" Height="450" Width="800"
        ResizeMode="NoResize"
        >
    <StackPanel Margin="10">
        <Label Height="39" Margin="20,10,694.6,0">
            <StackPanel Orientation="Horizontal">
                <AccessText Text="_Password"/>
            </StackPanel>
        </Label>
        <TextBox x:Name ="txtpassword" Height="32" Margin="20,0,162.6,0" />
        <Label Height="39" Margin="20,10,694.6,0">
            <StackPanel Orientation="Horizontal">
                <AccessText Text="_Email"/>
            </StackPanel>
        </Label>
        <TextBox x:Name ="txtemail" Height="32" Margin="20,0,162.6,0" />
        <Button Margin="216,50,369.6,50" Height="61" Content="slet" Click="slet"/>
        <StackPanel.Background>
            <ImageBrush ImageSource="billede\frontpage.png" Stretch="UniformToFill">
            </ImageBrush>
        </StackPanel.Background>

    </StackPanel>
</Window>
